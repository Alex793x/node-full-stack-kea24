{
    "version": 2,
    "builds": [
        {
            "src": "dist/app.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**", "public/**", "/node_modules/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/app.js"
        }
    ]
}